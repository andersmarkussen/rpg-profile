<h1>Search Results</h1>
<table>
  <thead>
    <tr>
      <th>Character Name</th><th>Modified</th>
    </tr>
  </thead>
  <tbody>
    {section name="i" loop="$characters"}
      <tr><td><a href="view.php?id={$characters[i].id}">{$characters[i].cname}</a></td><td>{$characters[i].lastedited}</td></tr>
    {sectionelse}
      <tr><td colspan="2">No characters found!</td></tr>
    {/section}
  </tbody>
</table>

{if $prevpage}
  <a href="search.php?type={$type}&amp;cname={$cname}&amp;page={$prevpage}">&lt; Previous</a>
  {if $nextpage}|{/if}
{/if}
{if $nextpage}
  <a href="search.php?type={$type}&amp;cname={$cname}&amp;page={$nextpage}">Next &gt;</a>
{/if}
